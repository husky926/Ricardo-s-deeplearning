   "source": [
    "import torch\n",
    "x = torch.tensor([2, 3, 4], dtype=torch.float, requires_grad=True)\n",
    "print(x)\n",
    "y = x * 2\n",
    "while y.norm() < 1000:\n",
    "    y = y * 2\n",
    "print(y)\n",
    "y.backward(torch.ones_like(y))\n",
    "print(x.grad)\n"
   ]
